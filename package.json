{
  "name": "instaview",
  "version": "1.0.0",
  "description": "simple instagram app",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start karma.config.js",
    "full-test": "npm run clean && npm install && npm run test",
    "clean": "rm -rf lib && rm -rf dist && rm -rf node_modules",
    "watch-js": "./node_modules/.bin/babel src -d lib --stage 1 --experimental -w",
    "dist-build": "./node_modules/.bin/babel src -d dist --stage 1 --experimental",
    "dev-build": "./node_modules/.bin/babel src -d lib --stage 1 --experimental",
    "dev-server": "node webpack.server.dev",
    "server": "nodemon lib/server/server",
    "eslint": "eslint -c .eslintrc src --quiet -f",
    "dist-webpack": "node ./node_modules/.bin/webpack --config webpack.config.prod",
    "dist-server": "NODE_ENV=prod node dist/server/server.js",
    "start": "npm run dev-build && cp src/index.jade lib/ && (npm run watch-js & npm run dev-server & npm run server)",
    "build": "npm run clean && npm install && npm run dist-build && npm run dist-webpack"
  },
  "engines": {
    "node": "4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abs2g08/instaview.git"
  },
  "author": "Adam Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abs2g08/instaview/issues"
  },
  "homepage": "https://github.com/abs2g08/instaview#readme",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "5.0.0-beta10",
    "babel-loader": "5.3.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "0.23.0",
    "escope": "3.3.0",
    "eslint": "1.6.0",
    "eslint-loader": "1.0.0",
    "eslint-plugin-babel": "2.2.0",
    "eslint-plugin-import": "0.7.9",
    "eslint-plugin-react": "3.5.1",
    "expect": "1.13.4",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.5",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-cli": "0.1.2",
    "karma-mocha": "0.2.1",
    "karma-sourcemap-loader": "0.3.6",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "nodemon": "1.7.1",
    "react-hot-loader": "1.3.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.12.0"
  },
  "dependencies": {
    "alt": "0.17.9",
    "axios": "0.8.0",
    "breakpoint-sass": "2.6.1",
    "classnames": "2.2.1",
    "compass-mixins": "0.12.7",
    "cookie-parser": "1.4.0",
    "express": "4.13.3",
    "express-session": "1.12.1",
    "history": "1.12.0",
    "instagram-node": "0.5.8",
    "iso": "5.0.0",
    "jade": "1.11.0",
    "moment": "2.10.6",
    "node-foundation-sass": "0.0.4",
    "react": "0.14.2",
    "react-dom": "0.14.2",
    "react-router": "1.0.0",
    "seamless-immutable": "5.1.0",
    "request": "^2.69.0"
  }
}
